# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true
tab_width = 4

[*.json]
indent_style = space
indent_size = 2

[*.csproj]
indent_style = space
indent_size = 2

[*.md]
max_line_length = off
trim_trailing_whitespace = false
